{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/charmainechin/FIT3179/main/data/100m_result.csv"},
    "vconcat": [{
      "width": 480,
      "mark": "line",
      "encoding": {
        "x": {
          "field": "datum.Year",
          "type": "temporal",
          "scale": {"domain": {"param": "brush"}},
          "axis": {"title": ""}
        },
        "y": {"field": "datum.Result", "type": "quantitative"}
      }
    }, {
      "width": 480,
      "height": 60,
      "mark": "area",
      "params": [{
        "name": "brush",
        "select": {"type": "interval", "encodings": ["x"]}
      }],
      "encoding": {
        "x": {
          "field": "datum.Year",
          "type": "temporal"
        },
        "y": {
          "field": "datum.Result",
          "type": "quantitative",
          "axis": {"tickCount": 3, "grid": false}
        }
      }
    }]
  }